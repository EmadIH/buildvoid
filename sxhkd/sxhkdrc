super + w
	chromium

super + g
	geany

super + s
	~/.local/bin/sysstate

super + c
	code-oss

super + m 
	~/.local/bin/mount-disk

super + u
	~/.local/bin/umount-disk

super + b
	~/.local/bin/blue-menu

# رفع الصوت
super + F10
    pulsemixer --change-volume +5 && killall -q slstatus && slstatus &
# خفض الصوت
super + F11
    pulsemixer --change-volume -5 && killall -q slstatus && slstatus &
# كتم أو إلغاء الكتم
super + F12
    pulsemixer --toggle-mute && killall -q slstatus && slstatus &
